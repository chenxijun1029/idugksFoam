FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      rho;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    top
    {
        type            bounceBack;
        value           nonuniform 0();
    }
    bottom
    {
        type            bounceBack;
        value           uniform 1;
    }
    left
    {
        type            cyclic;
    }
    right
    {
        type            cyclic;
    }
    frontAndBack
    {
        type            empty;
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform 1;
    }
    procBoundary1to0throughright
    {
        type            processorCyclic;
        value           uniform 1;
    }
    procBoundary1to3
    {
        type            processor;
        value           uniform 1;
    }
}


