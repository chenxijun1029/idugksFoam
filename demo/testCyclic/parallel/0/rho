FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      rho;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];

internalField   uniform 1.0;

boundaryField
{
    top
    {
        type            bounceBack;
        value           uniform 1.0;
    }
    bottom
    {
        type            bounceBack;
        value           uniform 1.0;
    }
    left
    {
        type            cyclic;
    }
    right
    {
        type            cyclic;
    }
    frontAndBackPlanes
    {
        type            empty;
    }
}


